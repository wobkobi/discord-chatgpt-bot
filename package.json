{
  "name": "chatgpt",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm/transpile-only src/index.ts\"",
    "start": "npm run build && node build/index.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --fix"
  },
  "dependencies": {
    "@discordjs/rest": "^2.5.1",
    "@giphy/js-fetch-api": "^5.7.0",
    "discord-api-types": "^0.38.19",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "logform": "^2.7.0",
    "luxon": "^3.7.1",
    "mathjax-full": "^3.2.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.2",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.34.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.2.1",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^57.0.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "overrides": {
    "undici": "7.10.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css,scss,html,json,yml,yaml,md,mdx}": "prettier --write"
  }
}
